
public class Exercício2 {

	public static void main(String[] args) {

	int a;
	int b;
	int c;
	
	if ( a> b) {
		
		if (a> c) {
			
			System.out.println("Maior valor é" + a);
		}else {
			System.out.println("Maior valor é " + c);
		}
		
	}else {
		
		if (b>c) {
			System.out.println("Maior valor:" + b);
		
		}else {
			System.out.println(" Maior valor: " + c);
		}
	}

}
